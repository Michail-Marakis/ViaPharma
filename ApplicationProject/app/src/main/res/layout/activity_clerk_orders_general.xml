<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#009688"
    tools:context=".view.ClerkOrdersGeneral.ClerkOrdersGeneral">

    <TextView
        android:id="@+id/textVieworderManage"
        android:layout_width="359dp"
        android:layout_height="41dp"
        android:text="@string/title_orders_manager"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.206" />

    <Button
        android:id="@+id/buttonALLord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_all_orders"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieworderManage" />

    <Button
        android:id="@+id/buttonbackord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_backorders"
        app:layout_constraintBottom_toTopOf="@+id/buttonALLord"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieworderManage" />

    <Button
        android:id="@+id/button7homepage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_home_page"
        app:icon="@drawable/home2"
        app:iconGravity="textStart"
        app:iconPadding="8dp"
        app:iconSize="23dp"
        tools:text="HOME PAGE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07" />

</androidx.constraintlayout.widget.ConstraintLayout>
